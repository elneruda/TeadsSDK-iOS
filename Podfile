project 'TeadsSampleApp.xcodeproj'

# Uncomment the next line to define a global platform for your project
# platform :ios, '10.0'

target 'TeadsSampleApp' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'TeadsSDK', :git => 'https://github.com/teads/TeadsSDK-iOS.git', :tag => 'v5.0.1'

  pod 'TeadsSASAdapter', :git => 'https://github.com/teads/TeadsSDK-iOS.git', :tag => 'v5.0.1'
  pod 'TeadsMoPubAdapter', :git => 'https://github.com/teads/TeadsSDK-iOS.git', :tag => 'v5.0.1'
  pod 'TeadsAdMobAdapter', :git => 'https://github.com/teads/TeadsSDK-iOS.git', :tag => 'v5.0.1'

end
